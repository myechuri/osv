{
    "apiVersion": "0.0.1",
    "swaggerVersion": "1.2",
    "basePath": "http://{{Host}}",
    "resourcePath": "/app",
    "produces": [
        "application/json",
        "application/xml"
    ],
    "apis": [
        {
            "path": "/app/",
            "operations": [
                {
                    "method": "PUT",
                    "summary": "Start an application",
                    "notes": "Run an application with its command line parameters. A space separated list of thread IDs is returned - one TID for each started application.",
                    "type": "string",
                    "nickname": "run_app",
                    "produces": [
                        "application/json",
                        "application/xml"
                    ],
                    "parameters": [
                       {
                                     "name": "command",
                                     "description": "command with its parameters",
                                     "required": true,
                                     "allowMultiple":false,
                                     "type":"string",
                                     "paramType":"query"
                               }
                    ]
                }
            ]
        }
    ]
}
